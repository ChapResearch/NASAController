                    android:layout_gravity="center_vertical|center"
                    android:fontFamily="@font/lato_regular"
                    android:gravity="center"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:text="#####"
                    android:textAlignment="center" />

                <RadioButton
