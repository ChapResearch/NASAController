                    android:fontFamily="@font/lato_regular"
                    android:gravity="center"
                    android:maxLength="9"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:text="ExampName"
                    android:textAlignment="center"
                    android:textSize="16sp" />

                <TextView
